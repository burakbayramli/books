    "# string_percent.ipynb\n",
    "Illustrate string formatting using the % method."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy as np\n",
    "\n",
    "print(\"The value of pi is approximately \" + str(np.pi))\n",
    "print(\"The value of %s is approximately %.5f\" % ('pi', np.pi))\n",
    "\n",
    "s = \"%d plus %d is %d\"\n",
    "print(s % (2, 4, 2 + 4))"
   ]
