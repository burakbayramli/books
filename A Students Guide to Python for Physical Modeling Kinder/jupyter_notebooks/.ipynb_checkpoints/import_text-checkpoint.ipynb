    "import numpy as np\n",
    "\n",
    "f = open(\"HIVseries.csv\")\n",
    "temp_data = []\n",
    "\n",
    "for line in f:\n",
    "\tprint(line)\n",
    "\tx,y = line.split(',')\n",
    "\ttemp_data += [ (float(x), float(y)) ]\n",
    "\n",
    "f.close()\n",
    "\n",
    "data_set = np.array(temp_data)"
